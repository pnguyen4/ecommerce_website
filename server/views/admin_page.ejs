<%- include("partials/header.ejs");                            _%>
<%- include("partials/nav.ejs");                               _%>
    <div class="container">
        <div class="row">
<%      for (let user of users) {                               %>
            <div class="col-lg-3 bg-light m-2">
            Username: <%= user.username %> <br>
            Email: <%= user.email %> <br>
            Favorites:
            <ul>
<%          for (let item of user.favorites) {                  %>
                <li>
                    Item: <%= item.name %><br>
                    Quantity in Stock: <%= item.quantity %><br>
                </li>
                <br>
            <% } %>
            </ul>
            </div>
        <% } %>

    </div>
<%- include("partials/footer.ejs");                            _%>
